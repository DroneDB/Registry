{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://192.168.2.251:5000",
      "sslPort": 0
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Default": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DDB_DEBUG": "1"
      },
      "commandLineArgs": "./registry-data --reset-hub",
      "applicationUrl": "http://localhost:5000"
    },
    "Default-Alt-Port": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DDB_DEBUG": "1"
      },
      "commandLineArgs": "./registry-data --reset-hub --address=localhost:7000",
      "applicationUrl": "http://localhost:7000"
    },
    "Default-All-Debug": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DDB_DEBUG": "1",
        "CPL_DEBUG":"ON",
        "CPL_LOG_ERRORS":"ON",
        "PROJ_DEBUG":"3"
      },
      "commandLineArgs": "./registry-data --reset-hub",
      "applicationUrl": "http://localhost:5000"
    },
    "WebServer": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "commandLineArgs": "./registry-data -t WebServer",
      "applicationUrl": "http://localhost:5000"
    },
    "Processing node": {
      "commandName": "Project",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "commandLineArgs": "./registry-data -t ProcessingNode"
    },
    "Thumbnail generator": {
      "commandName": "Project",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "commandLineArgs": "./registry-data -t ThumbnailGenerator -a localhost:5005"
    }
  }
}